        <p class="item__desc"><%=item.desc.substring(0,351)%>
            <b style="cursor : pointer;" onclick="modal({name : '<%=item.name%>' , description : `<%=item.desc%>`, price : '<%=item.price%>' , link : '<%=item.link%>'})">Read More</b> </p>

          <div class="buttons">
          <p class="item__price"><b><%=item.price%></b></p>
          <a href="<%=item.link%>" class="btn-light buy-btn">CHECK IT OUT</a>
        </div>
      </div>
      <% } %>
      <%})%>
    </div>
      <div class="loading-sec lifestyle-loading">
      <div class="container__load-more" id="container__load-more">
        <a class="btn-light btn-lg load-more" name="button" id="load-more" onclick="loadmore('Australasia')">LOAD MORE</a>
      </div>

      <p id="No-more" class="No-more">NO MORE ITEMS TO LOAD</p>
    </div>
  </div>
</div>
<div class="modal" id="modal">
  <div class="modal-content">
    <p class="close" onclick="modalClose()">x</p>
    <h1 class="title" id="title"></h1>
    <p class="description" id="description"></p>
    <div class="buttons" style="padding-bottom : 32px;">
      <p class="item__price"><b id=price>$100</b></p>
      <a href="" class="btn-light buy-btn" id="link">CHECK IT OUT</a>
    </div>
  </div>
</div>

  <div class="s_footer" style="background-color: rgba(50, 50, 50, 1)">
    <div style="width: 1370px; margin: 0px auto; margin-bottom: 20px">
      <div class="f_widgets">
        <ul>
          <li><a href="">Contact Us</a></li>
          <li><a href="">About Us</a></li>
          <li><a href="">Advertising</a></li>
        </ul>
      </div>
      <div class="f_widgets">
        <ul>
          <li><a href="">Privacy Policy</a></li>
          <li><a href="">Terms &amp; Conditions</a></li>
        </ul>
      </div>
      <br style="clear: both" />
    </div>
    <div style="width: 1370px; margin: 0px auto">
      <p
        style="
          margin: 0px;
          padding: 10px 0px;
          color: white;
          font-size: 15px;
          font-family: 'Arial';
        "
      >
        &copy; <%= new Date().getFullYear() %> all rights reserved.
      </p>
    </div>
  </div>

  <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script type="text/javascript" src="./js/experience.js">

    </script>
      <script type="text/javascript" src="./js/modal.js"></script>
      <script>
        $(document).ready(function() {
    $('#display-search').click(function() {
      $('#search-form').css('display', 'block');
      $('#close-search').css('display', 'block');
      $('#display-search').css('display', 'none');
    });
    $('#close-search').click(function() {
      $('#search-form').css('display', 'none');
      $('#close-search').css('display', 'none');
      $('#display-search').css('display', 'block');
    });

  });
      </script>
  </body>
</html>
