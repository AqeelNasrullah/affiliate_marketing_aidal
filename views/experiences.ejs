            <div class="buttons">
            <p class="item__price"><b><%=item.price%></b></p>
            <a href="<%=item.link%>" class="btn-light buy-btn">CHECK IT OUT</a>
          </div>
        </div>
        <% } %>
        <%})%>
      </div>
        
    </div>
  <div class="categoryy" id="America">
    
    <div class="content" id="content">
      <%items.forEach(item =>{%>
        <% if(item.sub_category == "America"){%>
