    <div class="shop-section" id="shop">
      <div class="heading" style="overflow-x: hidden;">
        <h2>Shop</h2>
        <p class="qa"><a href="#shop">Shop all Products  </a>
        </p>
      </div>

      <hr class="section">
      <div class="content" id="content">
        <%items.forEach(item =>{%>
        <div class="items">
          <img src="<%=item.thumbnail%>" alt="">
          <h4 style="margin:0;"><%=item.name%></h4>
          <p class="item__desc"><%=item.desc.substring(0,351)%>
             <b style="cursor : pointer;" onclick="modal({name : '<%=item.name%>' , description : `<%=item.desc%>`, price : '<%=item.price%>' , link : '<%=item.link%>'})">Read More</b> </p>

             <div class="buttons">
            <p class="item__price"><b><%=item.price%></b></p>
            <a href="<%=item.link%>" class="btn-light buy-btn">CHECK IT OUT</a>
          </div>
        </div>
        <%})%>
      </div>
      <div class="loading-sec">
      <div class="containerr__load-more" id="container__load-more">
        <a class="btn-light btn-lg load-more" name="button" id="load-more">LOAD MORE</a>
      </div>

      <p id="No-more">NO MORE ITEMS TO LOAD</p>
    </div>
    </div>

  </div>

