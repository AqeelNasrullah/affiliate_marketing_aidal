<link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">
    <style media="screen">
      body{
        font-family: 'Open Sans', sans-serif;
        background: #e0e0e0;
      }
    </style>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light p-4">
  <a class="navbar-brand" href="/">Home</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="/dashboard">Dashboard <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/dashboard/add">Add Item</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#" onclick="showthumb()">Change Thumbnail</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/dashboard/logout">Logout</a>
      </li>
    </ul>
    <form class="form-inline my-2 my-lg-0" method="post" action="/dashboard/search">
      <input class="form-control mr-sm-2" type="text" name="search"placeholder="Search" aria-label="Search">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
    </form>
  </div>
</nav>
<div class="container" style="display:block;">
  <form class="form-group my-4" action="/changethumbnail/<%=item_id%>" method="post" enctype="multipart/form-data" style="display : none;">
  <label for="image">Your Thumbnail : </label>
  <input type="file" name="image" class="mx-4" value="" id="image">
  <button type="submit" class="btn btn-primary subb"name="button" onclick="dis()">Upload</button>
</form>
  <div class="uploading" style="display:none;text-align : center;">
    <img src="./uploading.gif" alt="">
    <p>Please Wait while the files are being uploaded.</p>
  </div>
</div>
